{
    "name": "carloscgo/nova-google2fa",
    "description": "This package provides Google2FA to Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0|^8.0",
        "bacon/bacon-qr-code": "^2.0",
        "pragmarx/recovery": "^0.2.1",
        "pragmarx/google2fa-laravel": "^1.3.3"
    },
    "autoload": {
        "psr-4": {
            "CarlosCGO\\Google2fa\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarlosCGO\\Google2fa\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
